{
  "id": "webhook-example",
  "name": "Webhook Example",
  "description": "Example workflow triggered by webhook",
  "status": "active",
  "owner": "system",
  "created_at": "2024-01-01T00:00:00Z",
  "updated_at": "2024-01-01T00:00:00Z",
  "workflow_triggers": [
    {
      "id": "webhook-trigger-1",
      "name": "Webhook Trigger",
      "description": "HTTP webhook trigger for example workflow",
      "trigger_id": "webhook",
      "configuration": {
        "id": "example-webhook",
        "path": "/api/webhook/example"
      }
    }
  ],
  "steps": [
    {
      "id": "log-webhook-data",
      "name": "Log Webhook Data",
      "action_id": "log",
      "uid": "logwebhookdata",
      "configuration": {
        "message": "Received webhook data: {{trigger.body}}"
      },
      "conditional": {
        "language": "simple",
        "expression": "true"
      },
      "enabled": true
    },
    {
      "id": "transform-data",
      "name": "Transform Data",
      "action_id": "transform",
      "uid": "transformdata",
      "configuration": {
        "expression": "{ \"processed\": true, \"timestamp\": trigger.timestamp, \"method\": trigger.method, \"headers\": trigger.headers }"
      },
      "conditional": {
        "language": "simple",
        "expression": "true"
      },
      "enabled": true
    },
    {
      "id": "log-processed",
      "name": "Log Processed",
      "action_id": "log",
      "uid": "logprocessed",
      "configuration": {
        "message": "Processed webhook data: {{transformdata}}"
      },
      "conditional": {
        "language": "simple",
        "expression": "true"
      },
      "enabled": true
    }
  ],
  "variables": {},
  "metadata": {}
}