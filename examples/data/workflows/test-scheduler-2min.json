{
  "id": "test-scheduler-2min",
  "name": "Test Scheduler 2 Minute Interval",
  "description": "Test workflow for scheduler with 2-minute intervals to verify multi-schedule orchestrator",
  "nodes": [
    {
      "id": "scheduler-trigger-2min",
      "name": "2 Minute Schedule Trigger",
      "node_type": "trigger:scheduler",
      "category": "trigger",
      "config": {
        "cron_expression": "*/2 * * * *",
        "timezone": "UTC"
      },
      "enabled": true,
      "source_id": "test-scheduler-2min",
      "provider_id": "scheduler",
      "event_type": "schedule_due",
      "position_x": 50,
      "position_y": 100
    },
    {
      "id": "log-2min-trigger",
      "name": "Log 2-Minute Trigger",
      "config": {
        "message": "2-minute scheduler triggered at {{.trigger_data.published_at}} with cron: {{.trigger_data.cron_expression}}"
      },
      "position_x": 100,
      "position_y": 100,
      "enabled": true,
      "node_type": "log",
      "category": "action"
    },
    {
      "id": "process-2min-data",
      "name": "Process 2-Minute Schedule Data",
      "config": {
        "expression": "{\n  \"execution_time\": \"{{now}}\",\n  \"cron_expression\": \"{{.trigger_data.cron_expression}}\",\n  \"interval\": \"2-minutes\",\n  \"due_time\": \"{{.trigger_data.due_at}}\",\n  \"published_time\": \"{{.trigger_data.published_at}}\",\n  \"execution_count\": 1,\n  \"status\": \"success\"\n}"
      },
      "position_x": 300,
      "position_y": 100,
      "enabled": true,
      "node_type": "transform",
      "category": "action"
    },
    {
      "id": "log-2min-processed",
      "name": "Log 2-Minute Processed Data",
      "config": {
        "message": "{{ $data := index .node_results \"process-2min-data\" }}2-minute schedule execution #{{ $data.execution_count }} completed. Next execution in ~2 minutes."
      },
      "position_x": 500,
      "position_y": 100,
      "enabled": true,
      "node_type": "log",
      "category": "action"
    }
  ],
  "connections": [
    {
      "id": "trigger-scheduler-trigger-2min-to-log-2min-trigger",
      "source_port": "scheduler-trigger-2min:success",
      "target_port": "log-2min-trigger:main"
    },
    {
      "id": "trigger-to-process",
      "source_port": "log-2min-trigger:success",
      "target_port": "process-2min-data:main"
    },
    {
      "id": "process-to-log",
      "source_port": "process-2min-data:success",
      "target_port": "log-2min-processed:main"
    }
  ],
  "variables": {
    "execution_count": 0,
    "test_mode": true,
    "interval": "2-minutes"
  },
  "status": "active",
  "metadata": {
    "author": "Operion Test",
    "description": "Test workflow for multi-schedule orchestrator functionality",
    "tags": [
      "scheduler",
      "test",
      "multi-schedule",
      "2-minute"
    ],
    "version": "1.0"
  },
  "owner": "",
  "created_at": "2025-08-04T00:00:00.000000Z",
  "updated_at": "2025-08-04T00:00:00.000000Z"
}